+        private final int mSensorId;
+        private Callback mCallback;
+
+        HalResultController(int sensorId, Context context, Handler handler,
+                            BiometricScheduler scheduler, LockoutHalImpl lockoutTracker,
+                            LockoutResetDispatcher lockoutResetDispatcher) {
+            mSensorId = sensorId;
+            mContext = context;
